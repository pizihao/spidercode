<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mybatisderive.mapper.DeriveMapper">
    <select id="selectByCondition" resultType="com.example.mybatisderive.model.DO.DeriveDO">
        select *
        from derive
        where date = #{dto.id}
    </select>
</mapper>
